{
  "name": "airwave",
  "version": "1.0.0",
  "description": "Real-time aviation data mission control system powered by Airframes.io",
  "main": "backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node backend/kill-port.js && nodemon backend/server.js",
    "client": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "node backend/server.js",
    "test": "jest --coverage",
    "validate": "node backend/utils/schema-validator.js"
  },
  "keywords": [
    "aviation",
    "acars",
    "airframes",
    "mission-control",
    "real-time"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "axios": "^1.6.0",
    "better-sqlite3": "^12.4.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "find-process": "^1.4.7",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  }
}
